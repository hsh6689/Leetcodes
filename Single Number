//Leetcode 100 day Challenge Day-1

*/
  Resource:https://leetcode.com/explore/challenge/card/30-day-leetcoding-challenge/528/week-1/3283/
  Object: Given a non-empty array of integers, every element appears twice except for one. Find that single one.
  Running time: O(N), size of the array
/*

class Solution {
public:
    int singleNumber(vector<int>& nums) {
        int result=0;
        for(int i=0;i<nums.size();i++){
            result ^= nums[i];
        }
        return result;
    }
};
